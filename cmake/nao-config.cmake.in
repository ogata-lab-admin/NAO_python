# NAO CMake config file
#
# This file sets the following variables:
# NAO_FOUND - Always TRUE.
# NAO_INCLUDE_DIRS - Directories containing the NAO include files.
# NAO_IDL_DIRS - Directories containing the NAO IDL files.
# NAO_LIBRARIES - Libraries needed to use NAO.
# NAO_DEFINITIONS - Compiler flags for NAO.
# NAO_VERSION - The version of NAO found.
# NAO_VERSION_MAJOR - The major version of NAO found.
# NAO_VERSION_MINOR - The minor version of NAO found.
# NAO_VERSION_REVISION - The revision version of NAO found.
# NAO_VERSION_CANDIDATE - The candidate version of NAO found.

message(STATUS "Found NAO-@PROJECT_VERSION@")
set(NAO_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(NAO_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(NAO_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(NAO_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(NAO_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(NAO_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(NAO_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(NAO_DEFINITIONS ${<dependency>_DEFINITIONS})

set(NAO_VERSION @PROJECT_VERSION@)
set(NAO_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(NAO_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(NAO_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(NAO_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

